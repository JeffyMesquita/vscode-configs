{
    "editor.autoIndent": "full",
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorStyle": "line",
    "editor.fontSize": 16,
    "editor.fontWeight": "500",
    "editor.fontFamily": "Fira Code, Inconsolata",
    "editor.fontLigatures": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": false,
    "editor.letterSpacing": 0.3,
    "editor.lineHeight": 25,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "files.autoSave": "off",
    "search.showLineNumbers": true,
    "workbench.colorCustomizations": {},
    "workbench.iconTheme": "material-icon-theme",
  
    "indentRainbow.colors": [
      //ImobPower
      "#194B9022",
      "#86C44922",
      "#46D1FD22",
      "#777BB322"
      //Rainbow
      //   "#FF000022",
      //   "#FFA50022",
      //   "#FFFF0022",
      //   "#00880022",
      //   "#0000FF22",
      //   "#4B008222",
      //   "#EE82EE22"
      //RandomColor
      // "#1B998B22",
      // "#26465322",
      // "#FFFD8222",
      // "#FF9B7122",
      // "#E8485522"
    ],
  
    "zenMode.centerLayout": false,
    //ConfiguraÃ§Ã£o do terminal integrado
    "terminal.integrated.showExitAlert": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.defaultProfile.osx": "zsh",
    //Aumenta a fonte do Terminal
    "terminal.integrated.fontSize": 18,
    "terminal.integrated.profiles.osx": {
      "bash": {
        "path": "bash",
        "args": ["-l"],
        "icon": "terminal-bash"
      },
      "zsh": {
        "path": "/bin/zsh",
        "args": ["-l"]
      },
      "fish": {
        "path": "fish",
        "args": ["-l"]
      },
      "tmux": {
        "path": "tmux",
        "icon": "terminal-tmux"
      },
      "pwsh": {
        "path": "pwsh",
        "icon": "terminal-powershell"
      },
      "zsh/JeffyMesquita": {
        "path": "/bin/zsh",
        "args": ["-l"]
      }
    },
  
    "editor.rulers": [143, 183],
    "editor.wordWrapColumn": 143,
    // "eslint.validate": ["html", "javascript", "javascriptreact"],
  
    "material-icon-theme.folders.color": "#ff0000",
    "material-icon-theme.folders.theme": "specific",
    "material-icon-theme.opacity": 0.95,
    "material-icon-theme.saturation": 0.85,
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.files.associations": {
      "*.controller.ts": "nest-controller",
      "*.module.ts": "nest-module",
      "*.service.ts": "nest-service",
      "*.middleware.ts": "nest-middleware",
      "*.filter.ts": "nest-filter",
      "*.pipe.ts": "nest-pipe",
      "*.gateway.ts": "nest-gateway",
      "*.decorator.ts": "nest-decorator",
      "*.guard.ts": "nest-guard",
      "*.resolver.ts": "graphql",
      "*.input.ts": "graphql"
    },
    "material-icon-theme.folders.associations": {
      "components": "react-components",
      "insomnia": "json",
      "store": "other",
      "infra": "app",
      "application": "app",
      "entities": "class",
      "domain": "class",
      "schemas": "class",
      "typeorm": "database",
      "repositories": "mappings",
      "http": "container",
      "migrations": "tools",
      "modules": "components",
      "implementations": "core",
      "dtos": "typescript",
      "fakes": "mock",
      "websockets": "pipe",
      "protos": "pipe",
      "grpc": "pipe",
      "providers": "include",
      "subscribers": "messages",
      "useCases": "controller",
      "use-cases": "context",
      "inMemory": "database",
      "kafka": "scripts",
      "mappers": "meta",
      "_shared": "shared",
      "eslint-config": "tools"
    },
    "files.associations": {
      ".sequelizerc": "javascript",
      ".stylelintrc": "json",
      ".prettierrc": "json",
      "*.tsx": "typescriptreact"
    },
  
    "editor.renderLineHighlight": "gutter",
    "[javascript]": {
      "editor.formatOnSave": false,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "explorer.compactFolders": false,
  
    "emmet.syntaxProfiles": { "javascript": "jsx" },
    "emmet.includeLanguages": {
      "javascript": "javascriptreact"
    },
    "javascript.updateImportsOnFileMove.enabled": "never",
    "editor.parameterHints.enabled": false,
    "breadcrumbs.enabled": true,
    "javascript.suggest.autoImports": true,
    "[javascriptreact]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "explorer.confirmDragAndDrop": false,
    "[html]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "explorer.confirmDelete": false,
    "[typescriptreact]": {
      "editor.formatOnSave": false,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
      "editor.formatOnSave": false,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
  
    "[jsonc]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
  
    "prisma": {
      "editor.formatOnSave": true
    },
  
    "vsicons.dontShowNewVersionMessage": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    // Add those two lines:
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.inlineSuggest.enabled": true,
    "security.workspace.trust.untrustedFiles": "open",
  
    "tabnine.receiveBetaChannelUpdates": true,
    "tabnine.experimentalAutoImports": true,
  
    "cSpell.ignoreWords": ["Dani", "cardtype", "lintes", "mastercard"],
    "cSpell.enableFiletypes": [
      "!asciidoc",
      "!c",
      "!cpp",
      "!csharp",
      "!go",
      "!handlebars",
      "!haskell",
      "!jade",
      "!java",
      "!latex",
      "!php",
      "!pug",
      "!python",
      "!restructuredtext",
      "!rust",
      "!scala",
      "!scss"
    ],
    "cSpell.language": "en,pt-BR",
    "splitHTMLAttributes.closingBracketOnNewLine": true,
  
    "editor.minimap.size": "fit",
    "git.autofetch": true,
    "git.confirmSync": false,
    "gitlens.views.branches.showBranchComparison": "branch",
    "liveServer.settings.donotShowInfoMsg": true,
    // "github.copilot.enable": {
    //   "*": true,
    //   "yaml": true,
    //   "plaintext": true,
    //   "markdown": true,
    //   "json": true,
    //   "typescript": true,
    //   "javascript": true,
    //   "javascriptreact": true,
    //   "typescriptreact": true
    // },
    "[prisma]": {
      "editor.defaultFormatter": "Prisma.prisma"
    },
    "gitlens.views.branches.branches.layout": "list",
    "cSpell.userWords": [
      "Avenir",
      "cardtype",
      "Ciel",
      "firestore",
      "Jeferson",
      "Jeffy",
      "mastercard",
      "pokemon",
      "rocketseat",
      "shopify"
    ],
    "git.autofetchPeriod": 90,
    "git.showPushSuccessNotification": true,
    "gitHistory.avatarCacheExpiration": 90,
    "turboConsoleLog.delimiterInsideMessage": "=>",
    "turboConsoleLog.quote": "'",
    "turboConsoleLog.logMessagePrefix": "ðŸš€ðŸš€ðŸš€",
    "workbench.colorTheme": "Andromeda Italic Bordered",
    "[dockerfile]": {
      "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "vscode-pets.petColor": "red",
    "vscode-pets.theme": "castle",
    "gitlens.advanced.messages": {
      "suppressLineUncommittedWarning": true
    },
    "vscode-pets.position": "explorer",
    "vscode-pets.petType": "snake"
  }
  